@model Contact
@{
    ViewData["Title"] = "UpdateToContact";
    Layout = "~/Views/Layout/Index.cshtml";
}


<div class="content">
	<div class="container-fluid">
		<h4 class="page-title">İletişim</h4>
		<div class="row">
			<div class="col-md-12">
				<div class="card">
					<div class="card-body">
						<form method="post">
							<input type="hidden" asp-for="ContactId"/>
							<label>Başlık</label>
							<input type="text" asp-for="Title" class="form-control" />
							<br />
							<label>Açıklama</label>
							<input type="text" asp-for="Description" class="form-control" />
							<br />
							<label>Telefon1</label>
							<input type="text" asp-for="Phone1" class="form-control" />
							<br />
							<label>Telefon2</label>
							<input type="text" asp-for="Phone2" class="form-control" />
							<br />
							<label>Mail1</label>
							<input type="text" asp-for="Email1" class="form-control" />
							<br /><label>Mail2</label>
							<input type="text" asp-for="Email2" class="form-control" />
							<br /><label>Adres</label>
							<input type="text" asp-for="Address" class="form-control" />
							<br />
							<button class="btn btn-success">İletişimi Güncell</button>
						</form>

					</div>
				</div>
			</div>
		</div>
	</div>
</div>





